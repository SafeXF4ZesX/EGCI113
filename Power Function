#include <stdio.h>
#include <stdlib.h>
 //1. Function Prototype
void square (int);
void my_pow(int,int);

int main()
{
    my_pow(4,5);
    return 0;
}

void my_pow(int x,int y){
    int i,r;
    r=1;
    printf("%d^%d=",x,y);
    for(i=0;i<y;i++){
        r*=x;
    }
    printf("%d^%d=%d",x,y,r);
}

