#include <stdio.h>

int main()
{
    int a,b,c,max,min;
    printf("Input 3 numbers a b c : ");
    scanf("%d %d %d",&a,&b,&c);
    
    if(a>=b && a>=c) max=a;
    if(b>=c && b>=a) max=b;
    if(c>=a && c>=b) max=c;
    
    if(a<=b && a<=c) min=a;
    if(b<=c && b<=a) min=b;
    if(c<=a && c<=b) min=c;
    
    printf("The max is %d\n",max);
    printf("The min is %d\n",min);
    
    return 0;
}
